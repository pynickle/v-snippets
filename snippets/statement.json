{
    "fn": {
        "prefix": "fn",
        "body": [
            "fn ${1:name}() {",
            "    $2",
            "}"
        ],
        "description": "define a function"
    },
    "for": {
        "prefix": "for",
        "body": [
            "for ${1:initialize}; ${2:condition}; ${3:change}{",
            "    $4",
            "}"
        ],
        "description": "for loop like c"
    },
    "for...in": {
        "prefix": "for",
        "body": [
            "for ${1:item} in ${2:array}{",
            "    $3",
            "}"
        ],
        "description": "for loop with in arrays"
    },
    "mut": {
        "prefix": "mut",
        "body": [
            "mut ${1:name} = ${2:value}"
        ],
        "description": "mut keyword to initialize a changeable variable"
    },
    "if": {
        "prefix": "if",
        "body": [
            "if ${1:condition}{",
            "    $2",
            "}"
        ],
        "description": "if statement supported"
    },
    "if...else": {
        "prefix": "if",
        "body": [
            "if ${1:condition}{",
            "    $2",
            "} else {",
            "    $3",
            "}"
        ],
        "description": "if and else statement"
    },
    "if...else if...else": {
        "prefix": "if",
        "body": [
            "if ${1:condition}{",
            "    $2",
            "} else if ${3:condition}{",
            "    $4",
            "} else {",
            "    $5",
            "}"
        ],
        "description": "if, else if, else statement"
    },
    "import": {
        "prefix": "import",
        "body": [
            "import ${1:name}"
        ],
        "description": "import a module from zlib or local"
    },
    "struct": {
        "prefix": "struct",
        "body": [
            "struct ${1:name}{",
            "    ${2:name} ${3:type}",
            "}"
        ],
        "description": "struct a object"
    }
}