{
    "print": {
        "prefix": "print",
        "body": [
            "print(${1:object})"
        ],
        "description": "print an object without starting a new line"
    },
    "println": {
        "prefix": "println",
        "body": [
            "println(${1:object})"
        ],
        "description": "print an object with starting a new line"
    },
    "eprint": {
        "prefix": "eprint",
        "body": [
            "eprint(${1:object})"
        ],
        "description": "eprint support"
    },
    "eprintln": {
        "prefix": "eprintln",
        "body": [
            "eprintln(${1:object})"
        ],
        "description": "eprintln support"
    },
    "fn": {
        "prefix": "fn",
        "body": [
            "fn ${1:name}() {",
            "    $2",
            "}"
        ],
        "description": "define a function"
    },
    "mut": {
        "prefix": "mut",
        "body": [
            "mut ${1:name} = ${2:value}"
        ],
        "description": "mut keyword to initialize a changeable variable"
    },
    "if": {
        "prefix": "if",
        "body": [
            "if ${1:condition}{",
            "    $2",
            "}"
        ],
        "description": "if statement supported"
    },
    "if...else": {
        "prefix": "if",
        "body": [
            "if ${1:condition}{",
            "    $2",
            "} else {",
            "    $3",
            "}"
        ],
        "description": "if and else statement"
    },
    "if...else if...else": {
        "prefix": "if",
        "body": [
            "if ${1:condition}{",
            "    $2",
            "} else if ${3:condition}{",
            "    $4",
            "} else {",
            "    $5",
            "}"
        ],
        "description": "if, else if, else statement"
    }
}